{
  "name": "trs80-emulator",
  "version": "1.1.5",
  "description": "TRS-80 Model III emulator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tslint --project . --format codeFrame --exclude 'src/Decode*'",
    "lint:fix": "npm run lint -- --fix",
    "start": "rollup -c && http-server docs",
    "watch": "rollup -c --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "trs-80",
    "trs80",
    "emulator"
  ],
  "author": "Lawrence Kesteloot <lk@teamten.com> (https://www.teamten.com/lawrence/)",
  "repository": "https://github.com/lkesteloot/trs80-emulator",
  "license": "MIT",
  "dependencies": {
    "strongly-typed-events": "^2.0.6",
    "teamten-ts-utils": "^1.0.1",
    "trs80-base": "../trs80-base",
    "z80-base": "^1.0.6",
    "z80-emulator": "^1.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-typescript": "^2.0.1",
    "http-server": "^0.12.0",
    "rollup": "^1.27.14",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}
